<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间-->
<mapper namespace="cn.pxl.mybatisBasic.UserMapper">
    <!--标识开起二级缓存-->
    <cache/>
    <!--查询语句-->
    <select id="getUser" resultType="cn.pxl.entity.User">
        select * from User
    </select>

    <insert id="insertUser" parameterType="cn.pxl.entity.User">
        insert into User (id,userName,passWord,acctNbr,idNo,mobileNbr) values (#{id},#{userName},#{passWord},#{acctNbr},#{idNo},#{mobileNbr})
    </insert>
</mapper>
